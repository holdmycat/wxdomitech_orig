<view class="bgimg" 
      style="width:100%;height:320rpx;background-image:url({{upimgurl}});">
      <view class="shadow" 
            style="position:fixed;margin-left:5%;background-color:#fff;width:90%;height:250rpx;margin-top:70rpx;border-radius:50rpx;">
        <view class = 'logobody' style="margin-top:-7%;">
          <image class = 'logocss' src = '{{logourl}}' mode = 'aspectFit'/>
        </view>

        <view class="topic" style="font-size:30rpx;margin-top:-5%;">河北哆米艾特科技有限公司</view>
      </view>
</view>


<view style="margin-left:5%;margin-top:5%;position:fixed;width:90%;">

  <!-- 联系电话 -->
  <view style="display:flex">
    <view class="shadow2" 
      style="background-color:#fff; margin-bottom:6%;border-radius:10rpx;width:70%;height:120rpx">
      <view style="display:flex;">
      
        <image style="margin-left:22rpx;width:40rpx;height:45rpx;margin-right:4%;margin-top:50rpx;"
              src="{{registarray[1].iconurl}}"
              mode="aspectfit"
        >
        </image>
       
        <view class="text" 
              style="color:#000;line-height:400%;font-size:30rpx;width:50%;margin-top:44rpx;margin-left:-13rpx;"
        >
          <input placeholder='联系电话:0315-2172239' 
                placeholder-class="placeholder" 
                style="width:330rpx;"
                class="placeholder"
                id="1"
                maxlength='17'
                disabled='true'
                type='number'
                >
          </input>
        </view>    
      </view>
    </view>

    <view>
      <button  
          
          style="background-color:#3a9bf8;margin-left:15%;width:180rpx;height:120rpx;" 
          bindtap="tapphone">
          <view class="text" style="color:#fff;line-height:500%;">拨打</view>
      </button>
    </view>

  </view>
  


  <!--官网-->
  <view class="shadow2 " 
        style="margin-bottom:6%;border-radius:10rpx;height:120rpx">
    <view style="display:flex;">
    
      <image style="margin-left:22rpx;width:45rpx;height:50rpx;margin-right:4%;margin-top:50rpx;"
            src="{{registarray[0].iconurl}}"
            mode="aspectfit"
      >
      </image>
      
      <view class="text" 
            style="color:#000;line-height:400%;font-size:30rpx;width:50%;margin-top:44rpx;margin-left:-13rpx;"
      >
        <input placeholder='官网:www.domitech.com.cn' 
              placeholder-class="placeholder" 
              style="width:360rpx;
              class="placeholder"
              disabled='true'
              id="0"
              maxlength='10'
              type='text'
              >
        </input>
      </view>

      
    </view>
  </view>

  <!-- 商务联系人微信 -->
  <view class="shadow2 " 
        style="margin-bottom:6%;border-radius:10rpx;height:200rpx">
    <view style="display:flex;">
    
      <image style="margin-left:22rpx;width:45rpx;height:50rpx;margin-right:4%;margin-top:50rpx;"
            src="{{registarray[2].iconurl}}"
            mode="aspectfit"
      >
      </image>
      
      <view class="text" 
            style="color:#000;line-height:400%;font-size:30rpx;width:50%;margin-top:44rpx;margin-left:-13rpx;"
      >
        <input placeholder='商务联系人微信' 
              placeholder-class="placeholder" 
              style="width:360rpx;
              class="placeholder"
              disabled='true'
              id="0"
              maxlength='10'
              type='text'
              >
        </input>
      </view>

      <image style="margin-left:22rpx;width:200rpx;height:200rpx;margin-right:4%;margin-top:0rpx;"
            src="{{busimgurl}}"
            mode="aspectfit"
      >
      </image>
      
    </view>
  </view>

  <!--哆米业务微信公众号-->
<view class="shadow2 " 
        style="margin-bottom:6%;border-radius:10rpx;height:200rpx">
    <view style="display:flex;">
    
      <image style="margin-left:22rpx;width:45rpx;height:50rpx;margin-right:4%;margin-top:50rpx;"
            src="{{registarray[2].iconurl}}"
            mode="aspectfit"
      >
      </image>
      
      <view class="text" 
            style="color:#000;line-height:400%;font-size:30rpx;width:50%;margin-top:44rpx;margin-left:-13rpx;"
      >
        <input placeholder='哆米业务微信公众号' 
              placeholder-class="placeholder" 
              style="width:360rpx;
              class="placeholder"
              disabled='true'
              id="0"
              maxlength='10'
              type='text'
              >
        </input>
      </view>

      <image style="margin-left:22rpx;width:200rpx;height:200rpx;margin-right:4%;margin-top:0rpx;"
            src="{{busimgurl}}"
            mode="aspectfit"
      >
      </image>
      
    </view>
  </view>
</view>

<button class="shadow2" 
style="font-size:20rpx;margin-left:30%;position:fixed;bottom:5rpx;line-height:60rpx;background-color:#14a34b;color:#fff;width:240rpx;height:60rpx;" 
bindtap="taphome">
返回首页
</button>







