  <!-- 标题 -->
  <view >
    <!-- 横向图标 + 文字描述 -->
    <view class="layout_horizontal" style="padding-bottom:10px;">
      <view class = "image">
      <image  src='/images/logo-512.png' mode="aspectFit"></image>
      </view>
      <view class = "image, .td">
        <text style='font-size: 20px; padding-top:10px'>DOMITECH</text>
      </view>
    </view>
  </view>

  <!-- 产皮品类 -->
  <view>
    <view class='list'>
      <view class='item' wx:for="{{list}}">
        <view class = 'price' bindtap='selected' id = "{{item.id}}">
        <image style='width:40px;height:40px' src='{{item.imgurl}}'></image>  
        <view class="{{item.state?'underline':'default'}}, font2" >{{item.name}}</view>
        </view>   
      </view> 
    </view>
  </view>

<scroll-view 
      class = 'scrollPart'
      scroll-y = 'true'
      style="height:{{videoListHeight}}px;"
  >
    <view style = "padding-top:10px">
    <block wx:for="{{videoList}}" wx:key="{{index}}" wx:for-item="item">
      <view class="mod-item" style="height:{{videoItemInfo.height + 10}}px" >
        <view class="flex-wrp shadow" style="margin-left:5%;width:100%;height:{{videoItemInfo.height}}px;"bindtap='tapsucpro'  id = '{{item.id}}'>   

          <image 
                  style='margin-left:5px;margin-top:2px;width:{{videoItemInfo.width}}px;height:{{videoItemInfo.width/2}}px'
                  src="/images/vr1.png" >
          </image>

          <view style = "margin-left:5px;width:100%;font-size:10px;">VR Outdoor Cinem</view>
          
          <view style = "padding-top:1%;"></view>
          <view class='item' wx:for="{{list}}" style="margin-left:5px;">
            <image style='width:20px;height:20px' src='{{item.imgurl}}'></image>  
          </view> 


        </view>
      </view>
    </block>
    </view>
</scroll-view>

 

