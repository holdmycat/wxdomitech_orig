
.divLine{
  background: #cbd2dc;
  width: 100%;
  height: 1rpx;
 }

 
.page {
  background-color:#303238; 
height: 100%;
  overflow: hidden;
}

/*横向布局  */
.layout_horizontal{
  height: 100rpx;
  display: flex;
  /*row 横向  column 列表  */
  flex-direction: row;
  padding-bottom: 10px;
}

.td {
  display: flex;
  align-items: center;
  justify-content: center;
}


.list{
  border-top:1px solid #EDEDED; 
  display: flex;
  justify-content: space-between;  
  flex-wrap:wrap;
  box-sizing: content-box;
  width: 90%;
  padding-left: 5%;
}

.list2{
  border-top:1px solid #EDEDED; 
  display: flex;
  justify-content: center;  
  flex-wrap:wrap;
  box-sizing: content-box;  
  width: 100%;
 
}

.list .item{
  padding-top: 15px;
}
.price{
  flex-direction: column;
  margin:0 10px;
  
}

.font {
  display: block;
  text-align: center;
  width: 100%;
  font-size: 10px;
  color: #333;
  padding-top: 10px;
}


.font2 {
  display: block;
  text-align: center;
  width: 100%;
  font-size: 8px;
  color: #333;
  padding-top: 10px;
}
image {
  width: 60px;
  height:60px;
}
/*纵向布局  */
.layout_vertical{
  height: 100rpx;
  display: flex;
  /*row 横向  column 列表  */
  flex-direction: column;
}

.underline{ 
  padding-bottom: 1px;
  border-bottom: 1rpx solid #333;
}


